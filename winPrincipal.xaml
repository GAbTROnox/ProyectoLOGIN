<Window x:Class="winLogin.winPrincipal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:winLogin"
        mc:Ignorable="d"
        Title="winPrincipal" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label x:Name="lblBienvenido"
             Content="Bienvenido a mi sistema"
             HorizontalAlignment="Center"
             VerticalAlignment="Bottom"/>

        <Button x:Name="btnVolver"
            Grid.Row="1"
            Content="Volver"
            Margin="50,0" Click="btnVolver_Click"/>
    </Grid>
</Window>
